{
  "name": "sandstone",
  "version": "0.2.1",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "private": false,
  "author": "TheMrZZ - Florian ERNST",
  "description": "Sandstone, a Typescript library for Minecraft datapacks.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheMrZZ/sandstone.git"
  },
  "scripts": {
    "build:watch": "tsc --watch --incremental --pretty",
    "build": "node scripts/clean.js && tsc && node scripts/setupPackage.js",
    "test": "node dist/tests/test.js",
    "test:watch": "nodemon -q dist/tests/test.js",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^14.0.11",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "typescript": "^4.0.1-rc"
  },
  "dependencies": {
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.4",
    "sandstone": "^0.1.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "_moduleAliases": {
    "@arguments": "dist/src/_internals/arguments",
    "@commands": "dist/src/_internals/commands",
    "@datapack": "dist/src/_internals/datapack",
    "@variables": "dist/src/_internals/variables",
    "@": "dist/src/_internals"
  },
  "bugs": {
    "url": "https://github.com/TheMrZZ/sandstone/issues"
  },
  "homepage": "https://github.com/TheMrZZ/sandstone#readme",
  "directories": {
    "doc": "docs",
    "test": "tests",
    "lib": "src"
  }
}
