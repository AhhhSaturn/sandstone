{
  "name": "sandstone",
  "description": "Sandstone, a Typescript library for Minecraft datapacks.",
  "version": "0.14.0-alpha.12",
  "contributors": [
    {
      "name": "TheMrZZ - Florian ERNST",
      "url": "https://github.com/TheMrZZ"
    },
    {
      "email": "mulverin3@gmail.com",
      "name": "MulverineX",
      "url": "https://github.com/MulverineX"
    }
  ],
  "bugs": {
    "url": "https://github.com/sandstone-mc/sandstone/issues"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "graceful-fs": "^4.2.8",
    "nanoid": "^3.1.30",
    "nearley": "^2.20.1",
    "object-hash": "^2.2.0"
  },
  "devDependencies": {
    "@types/graceful-fs": "^4.1.5",
    "@types/module-alias": "^2.0.1",
    "@types/nearley": "^2.11.2",
    "@types/node": "^17.0.7",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "directories": {
    "doc": "docs",
    "test": "tests",
    "lib": "src"
  },
  "engines": {
    "node": ">=12.22.3"
  },
  "homepage": "https://sandstone-documentation.vercel.app/",
  "keywords": [
    "autocompletion",
    "creator",
    "datapack",
    "generator",
    "language",
    "library",
    "minecraft",
    "preprocessor",
    "typescript"
  ],
  "license": "MIT",
  "main": "dist/src/index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandstone-mc/sandstone.git"
  },
  "scripts": {
    "build": "tsc --noEmit && node scripts/clean.js && npm run fast-build",
    "fast-build": "ttsc && node scripts/setupPackage.js",
    "build-pack": "npm run fast-build && cd dist && npm pack",
    "watch": "nodemon --watch src --watch tests -e js,ts --exec \"npm run fast-build\" --pretty",
    "publishToNpm": "npm run build && npm publish dist",
    "format": "eslint src --cache --fix --quiet"
  },
  "types": "dist/src/index.d.ts"
}
